<h1>GL Accounts</h1>

<ul>
<%# change do block param to gl_account %>
  <% @gl_accounts.each do |account| %>
    <li>
      <%= account.number %>: <%= account.name %> <%= account.gl_type %> <%= link_to "Edit/Delete", account %>
    </li>
  <% end %>
</ul>

<%= button_to "Create new account", new_gl_account_path, :method => "get" %>
<br>
<%= button_to "Back to Home", root_path, :method => "get" %>

